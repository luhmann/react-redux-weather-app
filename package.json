{
  "name": "react-redux-weather-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "history": "^4.6.3",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hold": "^0.2.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-first-router": "^1.7.4",
    "redux-first-router-link": "^1.0.7",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint-config-prettier": "^2.2.0",
    "eslint-config-xo-react": "^0.12.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "nock": "^9.0.13",
    "prettier": "^1.5.0",
    "react-scripts": "1.0.7",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "xo": "^0.18.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "xo": {
    "extends": [
      "xo-react",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "semi": false
        }
      ]
    },
    "semicolon": false,
    "space": 2
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
